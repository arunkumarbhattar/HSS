   along with this program.  If not, see <http://www.gnu.org/licenses/>.  *//home/dhr33ti/Desktop/courses/hss/codeQL_assignment/autogen-5.18.7/autoopts/intprops.h_GL_BINARY_OP_OVERFLOW(a,b,op_result_overflow)op_result_overflow (a, b, _GL_INT_MINIMUM (0 * (b) + (a)), _GL_INT_MAXIMUM (0 * (b) + (a)))INT_LEFT_SHIFT_OVERFLOW(a,b)INT_LEFT_SHIFT_RANGE_OVERFLOW (a, b, _GL_INT_MINIMUM (a), _GL_INT_MAXIMUM (a))INT_REMAINDER_OVERFLOW(a,b)_GL_BINARY_OP_OVERFLOW (a, b, _GL_REMAINDER_OVERFLOW)INT_DIVIDE_OVERFLOW(a,b)_GL_BINARY_OP_OVERFLOW (a, b, _GL_DIVIDE_OVERFLOW)INT_MULTIPLY_OVERFLOW(a,b)_GL_BINARY_OP_OVERFLOW (a, b, _GL_MULTIPLY_OVERFLOW)INT_NEGATE_OVERFLOW(a)INT_NEGATE_RANGE_OVERFLOW (a, _GL_INT_MINIMUM (a), _GL_INT_MAXIMUM (a))INT_SUBTRACT_OVERFLOW(a,b)_GL_BINARY_OP_OVERFLOW (a, b, _GL_SUBTRACT_OVERFLOW)INT_ADD_OVERFLOW(a,b)_GL_BINARY_OP_OVERFLOW (a, b, _GL_ADD_OVERFLOW)_GL_UNSIGNED_NEG_MULTIPLE(a,b,max)(((b) < -_GL_SIGNED_INT_MAXIMUM (b) ? (_GL_SIGNED_INT_MAXIMUM (b) == (max) ? (a) : (a) % (_GL_INT_CONVERT (a, _GL_SIGNED_INT_MAXIMUM (b)) + 1)) : (a) % - (b)) == 0)_GL_REMAINDER_OVERFLOW(a,b,min,max)((min) < 0 ? (b) == _GL_INT_NEGATE_CONVERT (min, 1) && (a) < - (max) : (a) < 0 ? (a) % (b) != ((max) - (b) + 1) % (b) : (b) < 0 && ! _GL_UNSIGNED_NEG_MULTIPLE (a, b, max))_GL_DIVIDE_OVERFLOW(a,b,min,max)((min) < 0 ? (b) == _GL_INT_NEGATE_CONVERT (min, 1) && (a) < - (max) : (a) < 0 ? (b) <= (a) + (b) - 1 : (b) < 0 && (a) + (b) <= (a))_GL_MULTIPLY_OVERFLOW(a,b,min,max)(((min) == 0 && (((a) < 0 && 0 < (b)) || ((b) < 0 && 0 < (a)))) || INT_MULTIPLY_RANGE_OVERFLOW (a, b, min, max))_GL_SUBTRACT_OVERFLOW(a,b,min,max)((min) < 0 ? INT_SUBTRACT_RANGE_OVERFLOW (a, b, min, max) : (a) < 0 ? 1 : (b) < 0 ? (a) - (b) <= (a) : (a) < (b))_GL_ADD_OVERFLOW(a,b,min,max)((min) < 0 ? INT_ADD_RANGE_OVERFLOW (a, b, min, max) : (a) < 0 ? (b) <= (a) + (b) : (b) < 0 ? (a) <= (a) + (b) : (a) + (b) < (b))INT_LEFT_SHIFT_RANGE_OVERFLOW(a,b,min,max)((a) < 0 ? (a) < (min) >> (b) : (max) >> (b) < (a))INT_REMAINDE