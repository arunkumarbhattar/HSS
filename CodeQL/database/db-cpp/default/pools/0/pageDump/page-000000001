*   file name:  utf8.h
*   encoding:   UTF-8
*   tab size:   8 (not used)
*   indentation:4
*
*   created on: 1999sep13
*   created by: Markus W. Scherer
*/pipIsError/usr/include/unicode/utf16.hU16_SET_CP_LIMIT(s,start,i,length)UPRV_BLOCK_MACRO_BEGIN { if((start)<(i) && ((i)<(length) || (length)<0) && U16_IS_LEAD((s)[(i)-1]) && U16_IS_TRAIL((s)[i])) { ++(i); } } UPRV_BLOCK_MACRO_ENDU16_SET_CP_LIMIT_UNSAFE(s,i)UPRV_BLOCK_MACRO_BEGIN { if(U16_IS_LEAD((s)[(i)-1])) { ++(i); } } UPRV_BLOCK_MACRO_ENDU16_BACK_N(s,start,i,n)UPRV_BLOCK_MACRO_BEGIN { int32_t __N=(n); while(__N>0 && (i)>(start)) { U16_BACK_1(s, start, i); --__N; } } UPRV_BLOCK_MACRO_ENDU16_BACK_N_UNSAFE(s,i,n)UPRV_BLOCK_MACRO_BEGIN { int32_t __N=(n); while(__N>0) { U16_BACK_1_UNSAFE(s, i); --__N; } } UPRV_BLOCK_MACRO_ENDU16_BACK_1(s,start,i)UPRV_BLOCK_MACRO_BEGIN { if(U16_IS_TRAIL((s)[--(i)]) && (i)>(start) && U16_IS_LEAD((s)[(i)-1])) { --(i); } } UPRV_BLOCK_MACRO_ENDU16_BACK_1_UNSAFE(s,i)UPRV_BLOCK_MACRO_BEGIN { if(U16_IS_TRAIL((s)[--(i)])) { --(i); } } UPRV_BLOCK_MACRO_ENDU16_PREV_OR_FFFD(s,start,i,c)UPRV_BLOCK_MACRO_BEGIN { (c)=(s)[--(i)]; if(U16_IS_SURROGATE(c)) { uint16_t __c2; if(U16_IS_SURROGATE_TRAIL(c) && (i)>(start) && U16_IS_LEAD(__c2=(s)[(i)-1])) { --(i); (c)=U16_GET_SUPPLEMENTARY(__c2, (c)); } else { (c)=0xfffd; } } } UPRV_BLOCK_MACRO_ENDU16_PREV(s,start,i,c)UPRV_BLOCK_MACRO_BEGIN { (c)=(s)[--(i)]; if(U16_IS_TRAIL(c)) { uint16_t __c2; if((i)>(start) && U16_IS_LEAD(__c2=(s)[(i)-1])) { --(i); (c)=U16_GET_SUPPLEMENTARY(__c2, (c)); } } } UPRV_BLOCK_MACRO_ENDU16_PREV_UNSAFE(s,i,c)UPRV_BLOCK_MACRO_BEGIN { (c)=(s)[--(i)]; if(U16_IS_TRAIL(c)) { (c)=U16_GET_SUPPLEMENTARY((s)[--(i)], (c)); } } UPRV_BLOCK_MACRO_ENDU16_SET_CP_START(s,start,i)UPRV_BLOCK_MACRO_BEGIN { if(U16_IS_TRAIL((s)[i]) && (i)>(start) && U16_IS_LEAD((s)[(i)-1])) { --(i); } } UPRV_BLOCK_MACRO_ENDU16_SET_CP_START_UNSAFE(s,i)UPRV_BLOCK_MACRO_BEGIN { if(U16_IS_TRAIL((s)[i])) { --(i); } } UPRV_BLOCK_MACRO_ENDU16_FWD_N(s,i,length,n)UPRV_BLOCK_MACRO_BEGIN { int32_t __N=(n); while(__N>0 && ((i)<(length) || ((length)<0 && (s)[i]!=0))) { U16_FWD_1(s, i, length); --__N; } } UPRV_BLOCK_MACRO_ENDU16_FWD_N_UNSAFE(s,i,n)UPRV_BLOCK_MACRO_BEGIN { int32_t __N=(n); while(__N>0) { U16_FWD_1_UNSAFE(s, i); --__N; } } UPRV_BLOCK_MACRO_ENDU16_FWD_1(s,i,length)UPRV_BLOCK_MACRO_BEGIN { if(U16_IS_LEAD((s)[(i)++]) && (i)!=(length) && U16_IS_TRAIL((s)[i])) { ++(i); } } UPRV_BLOCK_MACRO_ENDU16_FWD_1_UNSAFE(s,i)UPRV_BLOCK_MACRO_BEGIN { if(U16_IS_LEAD((s)[(i)++])) { ++(i); } } UPRV_BLOCK_MACRO_ENDU16_APPEND(s,i,capacity,c,isError)UPRV_BLOCK_MACRO_BEGIN { if((uint32_t)(c)<=0xffff) { (s)[(i)++]=(uint16_t)(c); } else if((uint32_t)(c)<=0x10ffff && (i)+1<(capacity)) { (s)[(i)++]=(uint16_t)(((c)>>10)+0xd7c0); (s)[(i)++]=(uint16_t)(((c)&0x3ff)|0xdc00); } else { (isError)=true; } } UPRV_BLOCK_MACRO_ENDU16_APPEND_UNSAFE(s,i,c)UPRV_BLOCK_MACRO_BEGIN { if((uint32_t)(c)<=0xffff) { (s)[(i)++]=(uint16_t)(c); } else { (s)[(i)++]=(uint16_t)(((c)>>10)+0xd7c0); (s)[(i)++]=(uint16_t)(((c)&0x3ff)|0xdc00); } } UPRV_BLOCK_MACRO_ENDU16_NEXT_OR_FFFD(s,i,length,c)UPRV_BLOCK_MACRO_BEGIN { (c)=(s)[(i)++]; if(U16_IS_SURROGATE(c)) { uint16_t __c2; if(U16_IS_SURROGATE_LEAD(c) && (i)!=(length) && U16_IS_TRAIL(__c2=(s)[(i)])) { ++(i); (c)=U16_GET_SUPPLEMENTARY((c), __c2); } else { (c)=0xfffd; } } } UPRV_BLOCK_MACRO_ENDU16_NEXT(s,i,length,c)UPRV_BLOCK_MACRO_BEGIN { (c)=(s)[(i)++]; if(U16_IS_LEAD(c)) { uint16_t __c2; if((i)!=(length) && U16_IS_TRAIL(__c2=(s)[(i)])) { ++(i); (c)=U16_GET_SUPPLEMENTARY((c), __c2); } } } UPRV_BLOCK_MACRO_ENDU16_NEXT_UNSAFE(s,i,c)UPRV_BLOCK_MACRO_BEGIN { (c)=(s)[(i)++]; if(U16_IS_LEAD(c)) { (c)=U16_GET_SUPPLEMENTARY((c), (s)[(i)++]); } } UPRV_BLOCK_MACRO_ENDU16_GET_OR_FFFD(s,start,i,length,c)UPRV_BLOCK_MACRO_BEGIN { (c)=(s)[i]; if(U16_IS_SURROGATE(c)) { uint16_t __c2; if(U16_IS_SURROGATE_LEAD(c)) { if((i)+1!=(length) && U16_IS_TRAIL(__c2=(s)[(i)+1])) { (c)=U16_GET_SUPPLEMENTARY((c), __c2); } else { (c)=0xfffd; } } else { if((i)>(start) && U16_IS_LEAD(__c2=(s)[(i)-1])) { (c)=U16_GET_SUPPLEMENTARY(__c2, (c)); } else { (c)=0xfffd; } } } } UPRV_BLOCK_MACRO_ENDU16_GET(s,start,i,length,c)UPRV_