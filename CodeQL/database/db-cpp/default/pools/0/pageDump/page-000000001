 * Author: Daniel Veillard
 */dict/usr/include/libxml2/libxml/xmlregexp.h<libxml/dict.h><libxml/tree.h>xmlRegExecErrInfoconst xmlChar **xmlChar **xmlRegExecNextValuesxmlRegExecPushString2xmlRegExecPushStringxmlRegFreeExecCtxtxmlRegNewExecCtxtxmlRegexpIsDeterministxmlRegexpPrintxmlRegexpExecxmlRegFreeRegexpxmlRegexpCompilexmlRegExecCallbacksxmlRegExecCtxt *_xmlRegExecCtxt *xmlRegExecCtxtPtrxmlRegExecCtxt_xmlRegExecCtxtxmlRegexp *_xmlRegexp *xmlRegexpPtrxmlRegexp_xmlRegexp__XML_REGEXP_H__LIBXML_EXPR_ENABLED/*__XML_REGEXP_H__ *//* LIBXML_REGEXP_ENABLED *//* LIBXML_EXPR_ENABLED *//*
